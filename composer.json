{
    "name": "quartet/haydn",
    "description":" ",
    "keywords":[
    ],
    "license": "BSD-3-Clause",
    "repositories": {
        "quartet": {
            "type": "composer",
            "url": "http://dev.quartetcom.co.jp/composer"
        }
    },
    "config":{
        "bin-dir": "bin"
    },
    "autoload":{
        "psr-4":{
            "Quartet\\Haydn\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "Quartet\\Haydn\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "require": {
        "php": "~5.5",
        "quartet/common": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6"
    }
}
